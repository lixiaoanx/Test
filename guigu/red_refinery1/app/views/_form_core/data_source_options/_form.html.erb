<div class="form-group">
  <%= f.label :form_id %>
  <%= f.select :form_id, options_from_collection_for_select(@tenant.forms.all, :id, :title, options.form_id), { class_for_error: "is-invalid" }, class: "form-control" %>
  <%= f.error_message :form_id, class: "invalid-feedback" %>
</div>

<div class="form-group">
  <%= f.label :display_field_name %>
  <%= f.text_field :display_field_name, required: "required", class: "form-control", class_for_error: "is-invalid" %>
  <%= f.error_message :display_field_name, class: "invalid-feedback" %>
</div>
