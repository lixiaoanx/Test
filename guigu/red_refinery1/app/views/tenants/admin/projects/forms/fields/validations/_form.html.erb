<%= form_with(model: validations, scope: :validations, url: tenant_admin_project_form_field_validations_path(tenant, project, form, field), method: :patch, local: true) do |f| %>
  <% validations.class.attribute_names.each do |attribute_name| %>
    <%= render "_form_core/validations/#{attribute_name}", f: f, validations: validations %>
  <% end %>
  <% validations.class.reflections.keys.each do |attribute_name| %>
    <%= render "_form_core/validations/#{attribute_name}", f: f, validations: validations %>
  <% end %>

  <%= f.submit t("shared.form.submit.update"), class: "btn btn-block btn-primary" %>
<% end %>
